%clc
clear all
close all
%clc
dwt_without_optim = 1;
optim_fisher = 1;
optim_pce = 0;
optim_basis = 0;

addpath(genpath(cd));
rand('state',0);

sub_name = 'Subject 1 New';
filename = 'gediminas_unvoluntary.mat';
pathname = 'Signaux_EEG_imran/subject1_new/Unvoluntary/';
disp(sub_name);
figure('Name',sub_name);
programme_test_descripteur_xval(filename,pathname,dwt_without_optim,optim_fisher,optim_pce,optim_basis)
disp('-----------------------------------------------------------------');

